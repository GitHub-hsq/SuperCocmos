{
  "name": "chatgpt-web-service",
  "version": "1.0.0",
  "private": false,
  "description": "ChatGPT Web Service",
  "author": "ChenZhaoYu <chenzhaoyu1994@gmail.com>",
  "keywords": [
    "chatgpt-web",
    "chatgpt",
    "chatbot",
    "express"
  ],
  "engines": {
    "node": ">=16"
  },
  "scripts": {
    "start": "esno ./src/index.ts",
    "dev": "esno watch ./src/index.ts",
    "prod": "node ./build/index.mjs",
    "build": "pnpm clean && tsup",
    "clean": "rimraf build",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "common:cleanup": "rimraf node_modules && rimraf pnpm-lock.yaml",
    "test:db": "esno ./src/test-db.ts",
    "test:redis": "esno ./src/test-redis.ts",
    "test:llm": "esno ./test-llm-request.ts"
  },
  "dependencies": {
    "@langchain/community": "^0.3.8",
    "@langchain/core": "^0.3.11",
    "@langchain/langgraph": "^0.2.23",
    "@langchain/openai": "^0.3.17",
    "@supabase/supabase-js": "^2.75.0",
    "argon2": "^0.44.0",
    "axios": "^1.3.4",
    "chatgpt": "^5.1.2",
    "dotenv": "^16.0.3",
    "esno": "^4.7.0",
    "express": "^4.18.2",
    "express-jwt": "^8.5.1",
    "express-rate-limit": "^6.7.0",
    "https-proxy-agent": "^5.0.1",
    "ioredis": "^5.8.1",
    "isomorphic-fetch": "^3.0.0",
    "jwks-rsa": "^3.2.0",
    "langchain": "^0.3.35",
    "mammoth": "^1.6.0",
    "multer": "1.4.5-lts.2",
    "nanoid": "^5.1.6",
    "node-fetch": "^3.3.0",
    "pdf-parse": "^1.1.1",
    "socks-proxy-agent": "^7.0.0"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^5.4.1",
    "@types/express": "^4.17.17",
    "@types/multer": "^1.4.11",
    "@types/node": "^18.14.6",
    "eslint": "^9.17.0",
    "rimraf": "^4.3.0",
    "tsup": "^6.6.3",
    "typescript": "^4.9.5"
  }
}
